{
  "name": "mimmi20/ua-comparator",
  "description": "a little project to compare different useragent parser",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "Browser",
    "user agent",
    "http",
    "parser",
    "user-agent"
  ],
  "homepage": "https://github.com/mimmi20/ua-comparator",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/ua-comparator/graphs/contributors"
    }
  ],
  "support": {
    "issues": "https://github.com/mimmi20/ua-comparator/issues",
    "source": "https://github.com/mimmi20/ua-comparator"
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/mimmi20/browscap.git"
    }
  ],
  "require": {
    "php": "^7.0 | ^7.1",
    "symfony/console": "^3.0",
    "monolog/monolog": "^1.20",
    "devster/ubench": "^1.2",

    "browscap/browscap": "dev-detection-issues",

    "mimmi20/ua-result": "^2.0@dev",
    "mimmi20/ua-data-mapper": "^2.0@dev",

    "mimmi20/wurfl": "dev-update-to-version-1.8",
    "mimmi20/browser-detector": "dev-master",
    "mimmi20/browser-detector-version": "dev-master",
    "browscap/browscap-php": "dev-master",
    "crossjoin/browscap": "^1.0",
    "piwik/device-detector": "dev-master",
    "ua-parser/uap-php": ">=3.4.3,<4.0",
    "whichbrowser/parser": "dev-master",
    "woothee/woothee": "^1.2",

    "mimmi20/wurfl-generic-request": "dev-update-1.8",
    "mimmi20/wurfl-constants": "dev-update-1.8",
    "mimmi20/ua-normalizer": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.4",
    "squizlabs/php_codesniffer": "^2.5",
    "friendsofphp/php-cs-fixer": "^1.11",
    "cache/cache": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "UaComparator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UaComparatorTest\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --colors --verbose --coverage-text --coverage-clover=coverage.clover",
    "phpcs": "php -n vendor/bin/phpcs --standard=psr2 -np src tests web",
    "csfixer": "php -n -d memory_limit=768M vendor/bin/php-cs-fixer fix --dry-run -vv",
    "prepare": "./bin/prepare.sh",
    "init-server": "./bin/server.sh",
    "parse": "php bin/comparator parse --limit 10"
  }
}
