{
  "name": "mimmi20/ua-comparator",
  "description": "a little project to compare different useragent parsers",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "Browser",
    "user agent",
    "http",
    "parser",
    "user-agent"
  ],
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/ua-comparator/graphs/contributors"
    }
  ],
  "homepage": "https://github.com/mimmi20/ua-comparator",
  "support": {
    "issues": "https://github.com/mimmi20/ua-comparator/issues",
    "source": "https://github.com/mimmi20/ua-comparator"
  },
  "require": {
    "php": "~8.3.0 || ~8.4.0 || ~8.5.0",
    "ext-fileinfo": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "composer-runtime-api": "^2.0",
    "browscap/browscap-php": "^7.5.0",
    "cbschuld/browser.php": "^1.9.6",
    "clue/framework-x": "^0.17.0",
    "devster/ubench": "^2.1.0",
    "donatj/phpuseragentparser": "^1.10.0",
    "endorphin-studio/browser-detector": "^6.0.1",
    "endorphin-studio/browser-detector-data": "^1.2.0",
    "foroco/php-browser-detection": "^2.8.0",
    "fyre/useragent": "^2.1.1",
    "guzzlehttp/guzzle": "^7.9.2",
    "guzzlehttp/psr7": "^2.7.0",
    "hassankhan/config": "^3.2.0",
    "hexydec/agentzero": "^1.3.1",
    "jaybizzle/crawler-detect": "^1.3.3",
    "laminas/laminas-config-aggregator": "^1.18.0",
    "laminas/laminas-serializer": "^3.1.0",
    "laminas/laminas-servicemanager": "^4.4.0",
    "league/flysystem": "^3.29.1",
    "league/flysystem-local": "^3.29.0",
    "matomo/device-detector": "^6.4.5",
    "matthiasmullie/scrapbook": "^1.5.4",
    "mimmi20/browscap-helper-source": "^5.1.2",
    "mimmi20/browser-detector": "^10.0.2",
    "mimmi20/browser-detector-version": "^11.0.0",
    "mimmi20/monolog-callbackfilterhandler": "^4.0.0",
    "mimmi20/monolog-factory": "^5.0.0",
    "mimmi20/monolog-streamformatter": "^4.0.2",
    "mimmi20/ua-browser-type": "^11.0.0",
    "mimmi20/ua-data-mapper": "^4.0.0",
    "mimmi20/ua-result": "^12.0.0",
    "mobiledetect/mobiledetectlib": "^4.8.09",
    "monolog/monolog": "^3.8.1",
    "platine-php/user-agent": "^1.0.0",
    "psr/cache": "^3.0.0",
    "psr/container": "^1.1.2 || ^2.0.2",
    "psr/http-message": "^1.1 || ^2.0",
    "psr/log": "^3.0.2",
    "react/http": "^1.11.0",
    "symfony/console": "^7.2.1",
    "symfony/http-foundation": "^7.2.3",
    "ua-parser/uap-php": "^3.9.14",
    "whichbrowser/parser": "^2.1.8",
    "wolfcast/browser-detection": "^2.9.8",
    "woothee/woothee": "^1.10.0"
  },
  "require-dev": {
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "infection/infection": "^0.29.14",
    "mimmi20/coding-standard": "^6.0.15",
    "nikic/php-parser": "^5.4.0",
    "phpstan/extension-installer": "^1.4.3",
    "phpstan/phpstan": "^2.1.6",
    "phpstan/phpstan-deprecation-rules": "^2.0.1",
    "rector/rector": "^2.0.10",
    "rector/type-perfect": "^2.0.2",
    "shipmonk/composer-dependency-analyser": "^1.8.2",
    "symfony/process": "^7.2.4",
    "symplify/phpstan-rules": "^14.3.5",
    "tomasvotruba/cognitive-complexity": "^1.0.0",
    "tomasvotruba/type-coverage": "^2.0.2",
    "tomasvotruba/unused-public": "^2.0.0"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "UaComparator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UaComparatorTest\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "infection/extension-installer": true,
      "phpstan/extension-installer": true
    },
    "lock": false,
    "preferred-install": {
      "browscap/browscap": "source",
      "donatj/phpuseragentparser": "source",
      "jaybizzle/crawler-detect": "source",
      "matomo/device-detector": "source",
      "mimmi20/browser-detector": "source",
      "mobiledetect/mobiledetectlib": "source",
      "whichbrowser/parser": "source",
      "woothee/woothee-testset": "source",
      "*": "dist"
    },
    "sort-packages": true
  }
}
