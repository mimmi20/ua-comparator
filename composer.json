{
  "name": "mimmi20/ua-comparator",
  "description": "a little project to compare different useragent parsers",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "Browser",
    "user agent",
    "http",
    "parser",
    "user-agent"
  ],
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/ua-comparator/graphs/contributors"
    }
  ],
  "homepage": "https://github.com/mimmi20/ua-comparator",
  "support": {
    "issues": "https://github.com/mimmi20/ua-comparator/issues",
    "source": "https://github.com/mimmi20/ua-comparator"
  },
  "require": {
    "php": "^8.1.0",
    "ext-fileinfo": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "devster/ubench": "^2.1.0",
    "hassankhan/config": "^3.1.0",
    "league/flysystem": "^3.15.1",
    "matthiasmullie/scrapbook": "^1.5.1",
    "monolog/monolog": "^3.3.1",
    "symfony/console": "^v6.3.0"
  },
  "require-dev": {
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "mimmi20/coding-standard": "^5.0.5",
    "nikic/php-parser": "^4.15.5",
    "phpstan/extension-installer": "^1.3.1",
    "phpstan/phpstan": "^1.10.18",
    "phpstan/phpstan-deprecation-rules": "^1.1.3",
    "rector/rector": "^0.17.0",
    "vimeo/psalm": "^5.12.0"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "UaComparator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UaComparatorTest\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "lock": false,
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true,
      "infection/extension-installer": true
    }
  }
}
